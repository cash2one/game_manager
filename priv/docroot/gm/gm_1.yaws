<script type="text/javascript">
$(function(){
	$("#search").click(function(){
		var $userName = $("#name").val();
		var $uid = $("#uid").val();
		if( $userName == "" && $uid == "")
		{
			$("#message").text("请至少输入一项！");
			return false;
		}
		$("#message").text("");
		var $url = "../action.yaws?action=gm_1_1&username="+$userName+"&uid="+$uid;
		ajax( $url, "gm_1" )
      });
})
function callback( data, callback ){
